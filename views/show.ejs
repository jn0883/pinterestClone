<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/showstyle.css">
</head>

<body>
    <div class="nav">
        <div class="logo">
            <h3>Pinterest</h3>
        </div>
        <div class="features">
            <a href="/profile">Profile</a>
            <a href="/feed">Feed</a>
            <a class="logoutbtn" href="/logout">Logout</a>
        </div>
    </div>
    <div class="container">
        <% user.post.forEach(function(elem){ %> 
            <div class="cardcont">
                <div class="card">
                    <img src="/images/uploads/<%= elem.image %>" alt="">
                </div>
                <h5><%= elem.title %></h5>
            </div>
        <% }) %>    
    </div>
</body>

</html>